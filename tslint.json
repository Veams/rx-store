{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "jsRules": {},
	"rulesDirectory": ["tslint-plugin-prettier"],
	"rules": {
		"prettier": true,
        "interface-name": [true, "never-prefix"],
        "no-console": [true, "error"],
        "forin": false
	}
}
